{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off"
  },
  "ignorePatterns": ["demo.ts"],
  "overrides": [
    {
      "files": "*",
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "error",
          { "varsIgnorePattern": "^_" }
        ]
      }
    }
  ]
}
