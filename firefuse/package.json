{
  "name": "firefuse",
  "version": "3.0.0",
  "description": "Definitely typed utilities for firestore",
  "type": "module",
  "repository": "https://github.com/Hagihara-A/fire-fuse",
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc -w",
    "test": "npx jest",
    "lint": "npx eslint ./src && npx prettier ./src -c",
    "lint-fix": "npx eslint ./src --fix && npx prettier -w ./src",
    "test-w": "jest --watch",
    "prepublishOnly": "npm run build && npm pack",
    "release": "npx semantic-release"
  },
  "author": "arark",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "jest": "^27.2.2",
    "prettier": "^2.8.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "peerDependencies": {
    "firebase": "^9.0.0"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "firestore"
  ]
}
